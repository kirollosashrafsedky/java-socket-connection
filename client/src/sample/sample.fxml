<GridPane alignment="center" hgap="10" vgap="10" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.Controller">
   <columnConstraints>
      <ColumnConstraints />
   </columnConstraints>
   <rowConstraints>
      <RowConstraints />
   </rowConstraints>
   <children>
      <Pane prefHeight="516.0" prefWidth="514.0">
         <children>
            <Button fx:id="btnSend" layoutX="42.0" layoutY="280.0" mnemonicParsing="false" onAction="#btnSend_Handler" prefHeight="35.0" prefWidth="200.0" text="Send to server" />
            <TextArea fx:id="textArea" layoutX="42.0" layoutY="36.0" prefHeight="200.0" prefWidth="200.0" />
            <TextField fx:id="textField" layoutX="42.0" layoutY="246.0" prefHeight="25.0" prefWidth="200.0" />
            <Button fx:id="btnLogin" layoutX="373.0" layoutY="177.0" mnemonicParsing="false" onAction="#btnLogin_Handler" text="Login" />
            <TextField fx:id="passField" layoutX="322.0" layoutY="136.0" />
            <TextField fx:id="userField" layoutX="322.0" layoutY="90.0" />
            <Text layoutX="257.0" layoutY="107.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Username" />
            <Text layoutX="259.0" layoutY="153.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Password" />
